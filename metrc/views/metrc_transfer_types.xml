<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="metrc_transfer_type_tree" model="ir.ui.view">
        <field name="name">metrc.transfer.type.form</field>
        <field name="model">metrc.transfer.type</field>
        <field name="arch" type="xml">
            <tree string="Transfer Types" create="false">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="metrc_transfer_type_form" model="ir.ui.view">
        <field name="name">metrc.transfer.type.form</field>
        <field name="model">metrc.transfer.type</field>
        <field name="arch" type="xml">
            <form string="Transfer Types" create="false" edit="false">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="for_licensed_shipments"/>
                        <field name="external_incoming"/>
                        <field name="external_outgoing"/>
                        <field name="destination_grs_wt"/>
                        <field name="packages_grs_wt"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>