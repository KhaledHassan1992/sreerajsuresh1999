<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="metrc_customer_type_tree" model="ir.ui.view">
        <field name="name">metrc.customer.type.form</field>
        <field name="model">metrc.customer.type</field>
        <field name="arch" type="xml">
            <tree string="Customer Types" create="false">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="metrc_customer_type_form" model="ir.ui.view">
        <field name="name">metrc.customer.type.form</field>
        <field name="model">metrc.customer.type</field>
        <field name="arch" type="xml">
            <form string="Customer Types" create="false" edit="false">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>